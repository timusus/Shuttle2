<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of 'media directory selection' screen. User can choose to add directories to be scanned -->
    <string name="onboarding_directories_dialog_add_title">Katalogi mediów</string>
    <!-- Button to allow the user to add a directory, when using the 'S2 Media Scanner' -->
    <string name="onboarding_directories_dialog_add_button">Dodaj katalog</string>
    <!-- Error message title shown when the user is missing the 'document provider' app. (An app that allows them to choose files & directories) -->
    <string name="onboarding_directories_dialog_missing_title">Brak dostawcy dokumentów</string>
    <!-- Error message description shown when the user is missing the 'document provider' app. (An app that allows them to choose files & directories) -->
    <string name="onboarding_directories_dialog_missing_subtitle">Na urządzeniu nie można znaleźć aplikacji „Document Provider\” (menedżer plików). Konieczne może być zainstalowanie jednego lub powrót do korzystania z „podstawowego\” skanera multimediów.</string>
    <!-- Explanation shown to user when they're about to choose which directories wil lbe scanned bv the S2 media scanner-->
    <string name="onboarding_directories_add_directories_help_text">Aby skanować w poszukiwaniu muzyki, S2 wymaga dostępu do katalogów multimediów.\n\nPo naciśnięciu „dodaj katalog\” zostaniesz przeniesiony do domyślnej aplikacji „dostawcy plików\”. Przejdź do folderów zawierających Twoją muzykę. Możesz dodać wiele katalogów. \n\nUwaga: aby zobaczyć swoje foldery muzyczne, może być konieczne włączenie opcji „pokaż pamięć wewnętrzną\”.</string>
    <!-- Shown when the app is scanning the user's directories for media -->
    <string name="onboarding_directories_scanning">Skanowanie…</string>
    <!-- Shown when the media scanner is scanning the user's directories for media-->
    <plurals name="onboarding_directories_scan_progress">
        <item quantity="one">Plik audio 
      <xliff:g example="1" id="count">{count}</xliff:g></item>
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g>
       plików audio</item>
        <item quantity="few"><xliff:g example="30" id="count">{count}</xliff:g>
       plików audio</item>
        <item quantity="many"><xliff:g example="30" id="count">{count}</xliff:g>
       plików audio</item>
    </plurals>
    <!-- Dialog title shown to user to indicate that S2 requires permission to access their storage -->
    <string name="onboarding_permission_dialog_title">Wymagane pozwolenie na przechowywanie</string>
    <!-- Dialog message shown to explain that S2 requires permission to access their storage-->
    <string name="onboarding_permission_dialog_subtitle">S2 może nie działać poprawnie bez pozwolenia na dostęp do Twojej muzyki</string>
    <!-- Dialog button to grant permission to the app to access users storage-->
    <string name="onboarding_permission_button_grant">Dać pozwolenie</string>
    <!-- Shown after user initially denies access to storage -->
    <string name="onboarding_permission_help_text">Aby znaleźć Twoją muzykę, S2 potrzebuje uprawnień do odczytu pamięci masowej</string>
    <!-- Title of S2's Media Scanner, the thing that scans files on the device to read audio files/tags -->
    <string name="onboarding_media_scanner_title">Skaner mediów</string>
    <!-- Shown when the media scanner is scanning -->
    <string name="onboarding_media_scanner_scanning">Skanowanie…</string>
    <!-- Button to initiate a media scanner scan-->
    <string name="onboarding_media_scan">Skanowanie</string>
    <!-- Button to go back to the previous page, when onboarding -->
    <string name="onboarding_button_back">Z powrotem</string>
    <!-- Button to go to the next page, when onboarding -->
    <string name="onboarding_button_next">Kolejny</string>
    <!-- Button to trigger the MediaScanner to perform a rescan of audio files on the device-->
    <string name="onboarding_media_scanner_rescan">Skanuj ponownie</string>
    <!-- Shown when the S2 Media Provider is scanning song tags (metadata)-->
    <string name="onboarding_scanner_reading_song_tags">Czytanie tagów utworów…</string>
    <!-- Shown when user is onboarding. On this screen, user will choose their media provider, which will scan for their music-->
</resources>
